type Query {
  searchStudents(page: Int): SearchResultResponse
}

type Student {
  id: String
  name: String
  dateOfBirth: String
  gender: String
}

type Classes {
  id: String
  teacherName: String
  className: String
  studentList: [Student]
}

union SearchResult = Student | Classes

interface SearchResultResponse {
  data: [SearchResult]
  metaData: MetaData
  errorCode: String
  errorMessage: String
  timestamp: String
}

type MetaData {
  page: Int,
  size: Int,
}

